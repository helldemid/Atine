<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>Atine</title>

		<link rel="icon" href="{{ asset('media/favicon/favicon.ico') }}" sizes="any">
		<link rel="icon" type="image/png" sizes="16x16" href="{{ asset('media/favicon/favicon-16x16.png') }}">
		<link rel="icon" type="image/png" sizes="32x32" href="{{ asset('media/favicon/favicon-32x32.png') }}">
		<link rel="apple-touch-icon" sizes="180x180" href="{{ asset('media/favicon/apple-touch-icon.png') }}">
		<link rel="icon" type="image/png" sizes="192x192" href="{{ asset('media/favicon/android-chrome-192x192.png') }}">
		<link rel="icon" type="image/png" sizes="512x512" href="{{ asset('media/favicon/android-chrome-512x512.png') }}">
		<link rel="manifest" href="{{ asset('media/favicon/site.webmanifest') }}">

		<meta name="theme-color" content="#000000e6">

		<meta property="og:type" content="website">
		<meta property="og:url" content="{{ app.request.uri }}">

		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Hepta+Slab:wght@600;700&family=Inter:wght@300;400;600;800&family=Lora:ital,wght@0,400;0,500;0,700;1,400&display=swap" rel="stylesheet">
		{% block stylesheets %}
			<link rel="stylesheet" href="{{ asset('css/app.css') }}">
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">
			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/intl-tel-input@18.2.1/build/css/intlTelInput.min.css">
		{% endblock %}

		{% block javascripts %}
			<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js" defer></script>
			<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
			<script src="https://cdn.jsdelivr.net/npm/intl-tel-input@18.2.1/build/js/intlTelInput.min.js"></script>
			<script src="{{ asset('js/alertService.js') }}"></script>
			<script src="{{ asset('js/apiService.js') }}"></script>
			<script src="{{ asset('js/index.js') }}" defer></script>
		{% endblock %}
	</head>
	<body>
		{% block body %}{% endblock %}
	</body>
</html>
